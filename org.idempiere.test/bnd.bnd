-buildpath: \
	${junit},\
	org.assertj.core;version=latest,\
	${mockito},\
	${mockito-deps},\
	org.adempiere.base;version=latest,\
	org.eclipse.equinox.app;version=latest,\
	org.eclipse.osgi;version=latest,\
	org.junit.jupiter.api,\
	org.junit.platform.commons

javac.source: 1.8
javac.target: 1.8

Bundle-Version: 1.0.0.${tstamp}
Export-Package: \
	org.idempiere.test,\
	org.idempiere.test.assertions,\
	org.idempiere.test.generator,\
	org.idempiere.test.utils,\
	org.idempiere.test.junit4,\
	org.idempiere.test.junit5
Import-Package: \
	org.assertj.core.error,\
	!org.compiere.*,\
	!org.adempiere.*,\
	*
Require-Bundle: org.adempiere.base;bundle-version=6.2